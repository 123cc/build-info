      "recursive": "false",
      "regexp": "false",
      "excludePatterns": [""]
    },
    {
      "pattern": "files/a/b/*",
      "target": "${REPO}/${TEST_SPACE}/",
      "flat": "true",
      "recursive": "false",
      "regexp": "false",
      "excludePatterns": ["files/a/b/b1*"]
    },
    {
      "pattern": "files/a/b/c/*",
      "target": "${REPO}/${TEST_SPACE}/",
      "flat": "true",
      "recursive": "false",
      "regexp": "false",
      "excludePatterns": ["files/a/*ant*", "files/a/*c1.i?", "files/a/*c2"]
