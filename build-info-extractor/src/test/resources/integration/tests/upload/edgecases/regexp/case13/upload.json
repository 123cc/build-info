{
  "files": [
    {
      "pattern": "files/(.*)/(.*)\.in",
      "target": "${LOCAL_REPO}/{1}/{2}.in",
      "flat": "false",
      "recursive": "false",
      "regexp": "true"
    },
    {
      "pattern": "files/a/a1.in",
      "target": "${LOCAL_REPO}/canary",
      "flat": "false",
      "recursive": "false"
    }
  ]
}